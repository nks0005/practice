package AbstractWindowToolkit;

import java.awt.*;

public class PanelTest {
	public static void main(String[] args) {
		Frame f = new Frame("Panel test");
		f.setSize(300, 200);
		f.setLayout(null);
		
		Panel p = new Panel();
		p.setBackground(Color.green);
		p.setSize(100, 100);
		p.setLocation(50, 50);
		
		
	}
}
